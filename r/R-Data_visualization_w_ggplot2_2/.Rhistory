p3 <- p + stat_bin()
grid.arrange(p1,p2,p3, nrow=1)
grid.arrange(p1,p2,p3, nrow=3)
p <- ggplot(iris, aes(x = Sepal.Width))
p1 <- p + geom_histogram() + labs(title = "geom_histogram()")
p2 <- p + geom_bar() + labs(title = "geom_bar()")
p3 <- p + stat_bin() + labs(title = "stat_bin()")
grid.arrange(p1,p2,p3, nrow=3)
